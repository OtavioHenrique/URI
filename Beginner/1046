#include <iostream>
#include <cstdlib>
#include <iomanip>


using namespace std;
int main()
{
	int a, b, i;
	cin>>a>>b;
	if (a>b)
	{
		i=((24-a)+b);
		cout<<"O JOGO DUROU "<<i<<" HORA(S)"<<endl;
	}
	if (b>a)
		{
			i=(b-a);
			cout<<"O JOGO DUROU "<<i<<" HORA(S)"<<endl;
		}
	if (a==b)
	{
		cout<<"O JOGO DUROU 24 HORA(S)"<<endl;
	} 
	
	//system("pause");
	//cout<<a<<" "<<b;
	return(0);
}	
